{
  "name": "ts-templater",
  "version": "0.3.0",
  "description": "A flexible TypeScript library for dynamic string templating.",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/ts-templater.es.js",
      "require": "./dist/ts-templater.umd.js"
    },
    "./cache": {
      "types": "./dist/cache/index.d.ts"
    },
    "./functions": {
      "types": "./dist/functions/index.d.ts"
    },
    "./parser": {
      "types": "./dist/parser/index.d.ts"
    }
  },
  "scripts": {
    "build": "vite build",
    "test": "jest",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "typescript",
    "templating",
    "string",
    "data"
  ],
  "author": "Etzel Lava - S33 SRL<info@s33.it>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/S33-SRL/ts_templater"
  },
  "dependencies": {
    "bignumber.js": "^9.0.2",
    "dayjs": "^1.11.10"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "^11.1.6",
    "@types/bignumber.js": "^5.0.0",
    "@types/jest": "^29.5.12",
    "jest": "^29.3.1",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "tslib": "^2.6.2",
    "typescript": "^4.9.5",
    "vite": "^6.3.5"
  }
}